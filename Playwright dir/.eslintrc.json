{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "vitest-globals/env": true
    },
    "extends": ["airbnb", "plugin:vitest-globals/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "react/react-in-jsx-scope": "off",
        "react/button-has-type": "off",
        "no-alert": "off"
    }
}
