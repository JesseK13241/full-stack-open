{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "react/react-in-jsx-scope": "off",
        "react/button-has-type": "off",
        "no-alert": "off"
    }
}
